<%- include('./partials/head',{
    title : 'Login'
  }) %> 


<body class="register">
    <%- include('./partials/header') %> 


    <main class="register__main">
        <section class="register__main_section">
            <h1>Inicia Sesión</h1>
            <form class="form" action="">
                <div class="form__box">
                    <label class="form__label" for="email">Correo Electrónico</label>
                    <input class="form__input" type="email" id="email"  name="email">
                </div>
                <div class="form__box">
                    <label class="form__label" for="password">Contraseña</label>
                    <input class="form__input" type="password" id="password" name="password">
                </div>
                <div class="form__input_checkbox">
                    <input class="form__input_checkbox" type="checkbox">
                    <label >
                         Recúerdame
                    </label>
                </div>
                <div class="form__box_buttom">
                    <a class="form__button_cancel" href="">Cancelar</a>
                    <button class="form__button_submit" type="submit">Entrar</button>
                </div>
            </form>
        </section>
    </main>
      <%- include('./partials/footer') %> 

</body>
</html>